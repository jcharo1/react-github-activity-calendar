{
  "name": "react-github-activity-calendar",
  "version": "2.1.7",
  "keywords": [
    "react",
    "components",
    "ui",
    "responsive",
    "calendar",
    "react-activity-calendar",
    "react-github-calendar",
    "contributions-tracker",
    "github-activity",
    "github-contributions",
    "github"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcharo1/react-github-activity-calendar"
  },
  "dependencies": {
    "core-js": "^3.32.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "node-sass src/main.scss src/lib/components/style.comp.css",
    "compress:css": "node-sass src/lib/components/style.comp.css src/lib/components/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass compress:css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "author": {
    "name": "Justin Charo"
  },
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^5.0.1",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "webpack-cli": "^3.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
